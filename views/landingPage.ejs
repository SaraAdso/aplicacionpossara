<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" crossorigin="anonymous">
  <style>

  </style>
</head>

<body style="background-color: rgb(255, 255, 255);">

  <%- include ('parcials/navbar')%>

    <div id="carouselExampleIndicators" class="carousel slide" data-bs-ride="true">
      <div class="carousel-indicators">
        <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="0" class="active"
          aria-current="true" aria-label="Slide 1"></button>
        <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="1"
          aria-label="Slide 2"></button>
        <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="2"
          aria-label="Slide 3"></button>
      </div>
      <div class="carousel-inner">
        <div class="carousel-item active">
          <img
            src="https://karibik.vtexassets.com/assets/vtex.file-manager-graphql/images/24461397-0ffe-473c-bc0f-912aeb272d34___d3eb7d49fd5ede99c1b75b83662c7625.jpg"
            class="d-block w-100" alt="cargoobsession" width="100%">
        </div>
        <div class="carousel-item">
          <img
            src="https://karibik.vtexassets.com/assets/vtex.file-manager-graphql/images/11bbeb0f-3cd9-48c7-840e-4fd666679263___1d35e1475dc52e4d8277da7294f8a89e.jpg"
            class="d-block w-100" alt="cargoobsession" width="100%">
        </div>
        <div class="carousel-item">
          <img
            src="https://karibik.vtexassets.com/assets/vtex.file-manager-graphql/images/6d14ecb6-9e58-44ff-931a-62dda762aa81___d3ccd40653389a1d55041bd8f90d0249.jpg"
            class="d-block w-100" alt="...">
        </div>
      </div>
      <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleIndicators"
        data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
      </button>
      <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleIndicators"
        data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
      </button>
    </div>


    <h5 class="text-center m-4 fw-bold">CATEGORÍAS</h4>
      <div class="d-flex">
        <div class="w-50">
          <img class="img-fluid"
            src="https://karibik.vtexassets.com/assets/vtex.file-manager-graphql/images/cb4da983-d999-4023-89e3-a74c6f8a5634___054db00a4bc8ada7bc74ad78d5bf4cff.jpg"
            alt="">
        </div>
        
        <div class="w-50">
          <img class="img-fluid"
            src="https://karibik.vtexassets.com/assets/vtex.file-manager-graphql/images/3f01f528-1bda-4116-a529-986a1e95d649___507fd638e77501d7dbb6193834334e38.jpg"
            >
        </div>
      </div>


      <h5 class="text-center m-4 fw-bold">LO MÁS TOP</h5>
        <section class="d-flex flex-row flex-wrap justify-content-center">
          <% productos.slice(196,200).forEach(function(productito) { %>
            <div class="card m-4" style="width: 18rem;">
              <img src="https://picsum.photos/300/300?random=<%= productito._id %>" class="card-img-top" alt="...">
              <div class="card-body">
                <div class="d-flex justify-content-between">
                  <h5 class="card-title">
                    <%= productito.nombre %>
                  </h5>
                  <p class="card-text fw-bold">$<%= productito.precio %>
                  </p>
                </div>
                <p class="card-text">
                  <%= productito.descripcion %>
                </p>
                <button class="btn btn-warning"
                  onclick="agregarAlCarrito(`<%= productito._id %>`, `<%= productito.nombre %>`, `<%= productito.precio %>`)"><i
                    class="bi-cart-check"></i></button>
                <button type="button" class="btn btn-outline-dark" data-bs-toggle="modal"
                  data-bs-target="#modalinfo-<%= productito._id %>"> Ver
                  más</button>
              </div>
            </div>
            <div class="modal fade" id="modalinfo-<%= productito._id %>" tabindex="-1" aria-labelledby="modalactLabel"
              aria-hidden="true">
              <div class="modal-dialog modal-lg">
                <div class="modal-content">
                  <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                  <div class="mb-4 col-md-6 d-flex justify-content-evenly w-100">
                    <div>
                      <img src="https://picsum.photos/300/300?random=<%= productito._id %>" class="card-img-top"
                        alt="...">
                    </div>
                    <div class="modal-title text-center fs-5">
                      <h2 class="modal-title text-center fs-5" id="exampleModalLabel">
                        <%=productito.nombre %>
                      </h2>
                      <p class="fs-6 fw-lighter m-2 text-start">Ref: <%=productito.referencia %>
                      </p>
                      <p class="text-start fw-bold">$<%= productito.precio %>
                      </p>
                      <div class="btn-toolbar mb-3 d-flex justify-content-center" role="toolbar"
                        aria-label="Toolbar with button groups">
                        <div class="btn-group me-2" role="group" aria-label="First group">
                          <button type="button" class="btn btn-outline-secondary">XS</button>
                          <button type="button" class="btn btn-outline-secondary">S</button>
                          <button type="button" class="btn btn-outline-secondary">M</button>
                          <button type="button" class="btn btn-outline-secondary">L</button>
                          <button type="button" class="btn btn-outline-secondary">XL</button>
                          <button type="button" class="btn btn-outline-secondary">XXL</button>
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cerrar</button>
                    <button class="btn btn-warning"
                      onclick="agregarAlCarrito(`<%= productito._id %>`, `<%= productito.nombre %>`, `<%= productito.precio %>`)"><i
                        class="bi-cart-check"></i>Comprar</button>
                  </div>
                </div>
              </div>
            </div>
            </div>
            </div>
            <% }) %>
              <div class="d-flex align-items-center">
                <div class="centered-button padding-bottom 20px">
                  <a href="catalogo" class="btn btn-dark" role="button">MÁS PRODUCTOS</a>
                </div>
              </div>
        </section>
        <%- include ('parcials/carrito')%>

        <div>
          <img
            src="https://karibik.vtexassets.com/assets/vtex.file-manager-graphql/images/e0f3f9ee-6489-4a5e-abe5-94ad15197173___0079ca6602c36ac5d3b5d36dd7eb606c.jpg"
            class="d-block w-100 mt-4" alt="" width="100%">
          </div>

          <h5 class="text-center m-4 fw-bold">SOBRE NOSOTROS</h5>
          <div class="d-flex">
            <div class="w-50">
              <img class="img-fluid"
                src="https://karibik.vtexassets.com/assets/vtex.file-manager-graphql/images/7feae5e1-c840-4bd6-af7a-c9cf5513677e___d720b58ce072ab839e5fef3d93dfaf90.png"
                alt="" height="60px">
            </div>
            
            <div class="w-50">
              <img class="img-fluid"
                src="https://karibik.vtexassets.com/assets/vtex.file-manager-graphql/images/4b682bdb-6a36-4913-b2a7-f77fc349f3c0___ee3c9f4b67618a7d6c0e574ebb6c7cec.gif"
                >
            </div>
          </div>
          <!-- <button class="d-flex justify-content-evenly">VER MÁS</button>
          <a href="descargarexcel">Descargar</a> -->

          <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-kenU1KFdBIe4zVF0s0G1M5b4hcpxyD9F7jL+jjXkk+Q2h455rYXK/7HAuoJl+0I4"
            crossorigin="anonymous"></script>


</body>

</html>